<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="LLM.Interact.UI.MainWindow"
		xmlns:vm="using:LLM.Interact.UI.DTO"
		xmlns:c="clr-namespace:LLM.Interact.UI.Core"
		xmlns:views="using:LLM.Interact.UI"
		xmlns:loading="using:LLM.Interact.Components.Loading"
		mc:Ignorable="d"
		Width="900" Height="600"
		WindowStartupLocation="CenterScreen"
		CanResize="False"
		Title="LLM.Interact.UI"
		Icon="/Assets/ai-logo.ico"
		>
	<Grid x:Name="MainGrid">
		<views:HomePage />
		<loading:LoadingOverlay x:Name="loadingOverlay" IsVisible="False" />
	</Grid>
</Window>